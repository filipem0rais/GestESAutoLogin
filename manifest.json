{
    "name":"Autologin",
    "manifest_version": 3,
    "version":"1",
    "description":"Autologin for GestES",
    "content_scripts": [
        {
            "matches": ["https://www.gestes.info/gestes/"],
            "js": ["clickAccess.js"],
            "run_at": "document_end"
        },
        {
            "matches": ["https://www.gestes.info/gestes/connexion"],
            "js": ["esLogin.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "tabs",
        "contextMenus"
    ],
    "browser_action": {
        "default_title": "Your browser action title"
    },
    "host_permissions": [
        "https://www.gestes.info/*"
    ]
}